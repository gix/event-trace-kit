<Window x:Class="UIVirt.TableControlWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:etk="clr-namespace:UIVirt"
        xmlns:VsExtension="clr-namespace:EventTraceKit.VsExtension;assembly=EventTraceKit.VsExtension"
        mc:Ignorable="d"
        Title="MainWindow" Height="650" Width="1200">
  <Window.Resources>
    <Style x:Key="TraceLogGridCellTextStyle" TargetType="{x:Type TextBlock}">
      <Setter Property="Margin" Value="-6,0"/>
      <Setter Property="Padding" Value="4,0"/>
      <Setter Property="TextTrimming" Value="CharacterEllipsis"/>
    </Style>
  </Window.Resources>
  <Grid>
    <VsExtension:TraceLogWindowControl/>
  </Grid>
</Window>
